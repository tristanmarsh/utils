{
  "name": "@tristanmarsh/utils",
  "description": "Some pre-configured components to help with using framer-motion",
  "version": "1.0.2",
  "keywords": [
    "react",
    "framer motion",
    "mouse transform"
  ],
  "author": "Tristan Marsh <tristanmarsh@live.com> (https://tristanmarsh.net)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tristanmarsh/utils.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": "https://github.com/tristanmarsh/utils/issues",
  "module": "dist/utils.esm.js",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "storybook:deploy": "storybook-to-ghpages",
    "cm": "git-cz",
    "release": "np",
    "analyze": "source-map-explorer 'dist/*.js'"
  },
  "peerDependencies": {
    "@emotion/core": ">=10",
    "@emotion/styled": ">=10",
    "framer-motion": ">=2",
    "react": ">=16"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-docs": "^5.3.19",
    "@storybook/addon-info": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "@storybook/storybook-deployer": "^2.8.6",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "babel-loader": "^8.1.0",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "3.2.0",
    "framer-motion": "^2.4.1",
    "husky": "^4.2.5",
    "np": "^6.3.2",
    "react": "^16.13.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "ts-loader": "^8.0.0",
    "tsdx": "^0.13.2",
    "tslib": "^2.0.0",
    "typescript": "^3.9.6"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
